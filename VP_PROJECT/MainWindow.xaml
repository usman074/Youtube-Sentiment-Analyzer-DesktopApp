<Window x:Class="VP_PROJECT.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VP_PROJECT"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="600">
    <Grid>
        <StackPanel>
            <Label Content="Enter Youtube Video ID" Margin="10"/>
            <TextBox Name="id_txt" Height="23" TextWrapping="Wrap" Text="" Margin="10"/>
            <Label Content="Click Below Button For Whole Comments Analysis Of Video" Margin="10"/>
            <Button Name="whole_analysis" Content="Full Analysis" Margin="10,0,459,0" Height="35" Click="whole_analysis_Click" />
            <Label Content="Click Below Button For Individual Comments Analysis Of Video" Margin="10"/>
            <Button Name="individual_analysis" Content="Indvidual Analysis" Margin="10,10,459,0" Height="35" Click="individual_analysis_Click" />
        </StackPanel>
        

    </Grid>
</Window>
